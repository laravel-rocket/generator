{
  "name": "laravel-rocket/generator",
  "description": "Code Generator For Laravel Rocket",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Takaaki Mizuno",
      "email": "takaaki.mizuno@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "path",
      "url": "../foundation",
      "options": {
        "symlink": false
      }
    }
  ],
  "require": {
    "php": "^8.2",
    "symfony/yaml": "^7.0",
    "takaaki-mizuno/php-code-manipulator": "0.*",
    "takaaki-mizuno/swagger-parser": "0.1",
    "doctrine/dbal": "^4.0.1",
    "takaaki-mizuno/mwb-parser": "0.*",
    "nikic/php-parser": "^5.0",
    "ext-json": "*",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.1",
    "icanboogie/inflector": "^2.2",
    "laravel/framework": "^11.0",
    "laravel-rocket/foundation": "dev-master",
    "friendsofphp/php-cs-fixer": "^3.25",
    "fakerphp/faker": "^1.23",
    "laravel/laravel": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "LaravelRocket\\Generator\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ]
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
